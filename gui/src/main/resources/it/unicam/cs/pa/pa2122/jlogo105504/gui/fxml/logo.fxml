<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="700.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unicam.cs.pa.pa2122.jlogo105504.gui.controller.LogoController">
   <children>
      <VBox layoutY="78.0" prefHeight="623.0" prefWidth="914.0" stylesheets="@../css/styling.css">
         <children>
            <Canvas height="600.0" width="800.0">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </VBox.margin>
            </Canvas>
         </children></VBox>
      <HBox prefHeight="79.0" prefWidth="920.0" styleClass="hbox" stylesheets="@../css/styling.css">
         <children>
            <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="84.0" styleClass="open-file-button" stylesheets="@../css/styling.css" text="Run All">
               <HBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </HBox.margin></Button>
            <Button layoutX="25.0" layoutY="25.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="78.0" styleClass="open-file-button" stylesheets="@../css/styling.css" text="Clear">
               <HBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </HBox.margin></Button>
            <Button layoutX="108.0" layoutY="25.0" mnemonicParsing="false" onAction="#changeSizePanel" prefHeight="30.0" prefWidth="86.0" styleClass="open-file-button" stylesheets="@../css/styling.css" text="Change Size">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <HBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </HBox.margin></Button>
            <Button layoutX="208.0" layoutY="25.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="86.0" styleClass="open-file-button" stylesheets="@../css/styling.css" text="Save File">
               <HBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </HBox.margin></Button>
            <Button layoutX="399.0" layoutY="30.0" mnemonicParsing="false" onAction="#tryToOpenFile" prefHeight="30.0" prefWidth="86.0" styleClass="open-file-button" stylesheets="@../css/styling.css" text="Open File" />
            <Button layoutX="505.0" layoutY="10.0" mnemonicParsing="false" onAction="#setCloseJLogo" prefHeight="30.0" prefWidth="86.0" styleClass="open-file-button" stylesheets="@../css/styling.css" text="Close" />
            <Button fx:id="playButton" layoutX="80.0" layoutY="1.0" mnemonicParsing="false" onAction="#startExecution" prefHeight="36.0" prefWidth="86.0" text="PLAY" />
         </children>
      </HBox>
   </children>
</AnchorPane>
